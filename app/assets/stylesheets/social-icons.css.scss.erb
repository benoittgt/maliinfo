@font-face {
  font-family: "Social Icon";
  src: url('<%= asset_path("social-icons.eot") %>');
  src: url('<%= asset_path("social-icons.eot") + "?#iefix" %>') format('eot'), url('<%= asset_path("social-icons.woff") %>') format('woff'), url('<%= asset_path("social-icons.ttf") %>') format('truetype'), url('<%= asset_path("social-icons.svg") + "#Social Icon" %>') format('svg');
  font-weight: normal;
  font-style: normal;
}